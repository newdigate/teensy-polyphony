{
    "boost-test-adapter.logpanel": true,
    "boost-test-adapter.testExecutable": "cmake-build-debug/test/tests",
    "cmake.buildDirectory": "${workspaceFolder}/cmake-build-${variant:buildType}",
    "cmake.configureSettings": {
        "CMAKE_BUILD_TYPE": "${buildType}"        
    },
    "boost-test-adapter.cwd": "${workspaceFolder}",
    "cmake.generator": "Unix Makefiles",
    "files.associations": {
        "deque": "cpp",
        "string": "cpp",
        "vector": "cpp"
    }
}